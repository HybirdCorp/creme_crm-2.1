{
    "title": {
        "text":       "<b>{{rgraph}}</b>",
        "textColor":  "black",
        "fontSize":   "13pt",
        "renderer":   "jqplot.DivTitleRenderer"
    },
    "seriesDefaults": {
        "renderer": "jqplot.PieRenderer", 
        "rendererOptions": {
            "showDataLabels": true,
            "shadow": false,
            "padding": 5,
            "dataLabelFormatString": "<span style='color:black'>%1$.2f %%</span>"
        }
    },
    "legend": {
        "show": true,
        "renderer": "jqplot.EnhancedLegendRenderer",
        "rendererOptions": {
            "numberRows": 1,
            "disableIEFading": false,
            "fontSize":   "9pt"
        },
        "labels": "preprocess.formatSerieLabel",
        "formatSerieLabelOptions": {
            "format": "<span style='color:black;'>%0$s&nbsp;&nbsp;<span style='float:right;'>%3$.2f&nbsp;%%</span></span>"
        },
        "location": "e"
    },
    "grid": {
        "shadow": false,
        "background": "#ffffff",
        "drawBorder": false
    },
    "highlighter": {
        "show": true,
        "showMarker": false,
        "useAxesFormatters": false,
        "tooltipAxes": "xy",
        "formatString": "<center>%s</center><center>{% if rgraph.is_count %}%d{% else %}%.2f{% endif %}</center>",
        "tooltipLocation": "cursor",
        "cursorStyle": "pointer"
    },
    "handlers": [
        {"action": "redirect", "event": "click", "url": "%2$s"}
    ],
    "dataFormat": "creme.graphael.BargraphData",
    "dataPreprocessors": [{
                              "preprocessor": "percentEntry",
                              "options": {
                                  "valueIndex": 1,
                                  "targetIndex": 3
                              }
                          }
                         ]
}